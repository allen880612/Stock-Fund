<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="android.example.final_2">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="android.example.final_2.BackTesting"
            android:parentActivityName="android.example.final_2.Description"></activity>
        <activity
            android:name="android.example.final_2.Description"
            android:parentActivityName="android.example.final_2.Load" />
        <activity
            android:name="android.example.final_2.MainActivity"
            android:parentActivityName="android.example.final_2.Description" />
        <activity android:name="android.example.final_2.Load">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>